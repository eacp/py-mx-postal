{
  "hosting": [{
    "target": "data",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "*.py"
    ],

    "headers": [ {
      "source": "**/*.@(json)",
      "headers": [ {
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      },{"key": "Cache-Control","value": "max-age=864000"}]
    } ]


  }]
}
